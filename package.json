    "playwright": "1.50.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3"
  },
  "scripts": {
    "debug": "ts-node src/Debug/Debugger.ts",
    "test:exec": "ts-node src/execution.ts",
    "test:eval": "ts-node src/evaluation.ts",
    "test:inspect": "ts-node src/inspection.ts",
    "test:allure": "npx ts-node src/Report/generate_allure.ts && allure generate allure-results --clean -o allure-report && allure serve allure-results",
